{
  "name": "react-base-starter",
  "version": "0.0.1",
  "description": "React Base Starter Kit",
  "authors": [
    "Pablo Magaz (@pmagaz)",
    "José Bermudo (@josemanuelbd)",
    "Alberto Martín (@albertoMartin1988)",
    "Rafael Pérez (@rperez.cognizant)"
  ],
  "private": true,
  "scripts": {
    "postinstall": "npm run clean",
    "logo": "ts-node ./src/base/conf/.react-base/logo.ts",
    "clean": "rimraf dist && rimraf .cache",
    "prettier": "prettier --write './src/**/*.{js,jsx,ts,tsx}'",
    "check-integrity": "npm run lint && npm run types && npm run test",
    "lint": "npm run logo && eslint src --ext .ts --ext .tsx --ext .js --ext .jsx",
    "types": "tsc --noEmit",
    "test": "jest",
    "test:coverage": "jest --coverage",
    "build": "npm run logo && npm run clean && cross-env NODE_ENV=production parcel build src/app/index.html -d dist --no-cache",
    "start": "npm run logo && cross-env NODE_ENV=development ts-node server",
    "start:prod": "npm run build && cross-env NODE_ENV=production ts-node server"
  },
  "lint-staged": {
    "./src/**/*.{js,jsx,ts,tsx}": [
      "npm run lint"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && npm run types",
      "pre-push": "npm run test && npm run build",
      "commit-msg": "echo ⚠️ THINK BEFORE ACTING AND REMEMBER, LESS IS MORE. KISS!! "
    }
  },
  "keywords": [
    "react",
    "redux",
    "react-router",
    "immutableJS",
    "webpack"
  ],
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.9.0",
    "@babel/plugin-proposal-class-properties": "7.8.3",
    "@babel/plugin-syntax-dynamic-import": "7.8.3",
    "@babel/plugin-transform-runtime": "7.9.0",
    "@babel/preset-env": "7.9.0",
    "@babel/preset-react": "7.9.4",
    "@babel/preset-typescript": "^7.7.7",
    "@types/enzyme": "3.10.5",
    "@types/enzyme-adapter-react-16": "1.0.6",
    "@types/express": "4.17.3",
    "@types/jest": "25.1.4",
    "@types/node": "^13.1.6",
    "@types/parcel-bundler": "^1.12.1",
    "@types/react": "16.9.27",
    "@types/react-dom": "16.9.5",
    "@types/react-redux": "7.1.7",
    "@types/react-router-dom": "^5.1.3",
    "@types/redux-logger": "^3.0.7",
    "@types/styled-components": "5.0.1",
    "@types/webpack-env": "1.15.1",
    "@typescript-eslint/eslint-plugin": "2.25.0",
    "@typescript-eslint/parser": "2.25.0",
    "babel-eslint": "10.1.0",
    "babel-jest": "25.2.4",
    "babel-plugin-module-resolver": "4.0.0",
    "babel-plugin-styled-components": "1.10.7",
    "cross-env": "7.0.2",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "enzyme-to-json": "3.4.4",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.1.0",
    "eslint-config-prettier": "6.10.1",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jest": "^23.4.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.19.0",
    "eslint-plugin-react-hooks": "3.0.0",
    "esm": "3.2.25",
    "husky": "4.2.3",
    "jest": "25.2.4",
    "lint-staged": "10.1.0",
    "parcel-plugin-bundle-visualiser": "1.2.0",
    "prettier-eslint": "9.0.1",
    "redux-devtools-extension": "2.13.8",
    "redux-logger": "3.0.6",
    "rimraf": "3.0.2",
    "typescript": "3.8.3"
  },
  "dependencies": {
    "axios": "0.19.2",
    "core-js": "3.6.4",
    "create-reduxreducer": "1.0.0",
    "express": "4.17.1",
    "history": "4.10.1",
    "immer": "6.0.2",
    "parcel-bundler": "1.12.4",
    "parcel-plugin-eslint": "1.0.7",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-redux": "7.2.0",
    "react-router-dom": "5.1.2",
    "redux": "4.0.5",
    "redux-req-middleware": "1.0.5",
    "styled-components": "5.0.1",
    "ts-node": "8.8.1"
  },
  "engines": {
    "node": "10.x"
  }
}
